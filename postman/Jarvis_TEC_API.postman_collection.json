  "item": [
    {
      "name": "Health & Status",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/health",
              "host": ["{{base_url}}"],
              "path": ["api", "health"]
            },
            "description": "Verifica el estado de salud del sistema y servicios disponibles"
          },
          "response": [
            {
              "name": "Success Response",
              "originalRequest": {
                "method": "GET",
                "url": "{{base_url}}/api/health"
              },
              "status": "OK",
              "code": 200,
              "_postman_previewlanguage": "json",
              "body": "{\n  \"status\": \"healthy\",\n  \"timestamp\": \"2025-10-15T12:00:00Z\",\n  \"services\": {\n    \"face_api\": \"available\",\n    \"speech_api\": \"available\",\n    \"ml_models\": \"loaded\"\n  }\n}"
