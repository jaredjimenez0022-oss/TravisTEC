                {
                  "key": "image",
                  "type": "file",
                  "src": "/path/to/image.jpg",
                  "description": "Imagen con rostro a analizar"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/v1/face/sentiment",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "face", "sentiment"]
            },
            "description": "Detecta rostros y analiza emociones en una imagen"
          },
          "response": [
            {
              "name": "Success - Face Detected",
              "originalRequest": {
                "method": "POST",
                "url": "{{base_url}}/api/v1/face/sentiment"
